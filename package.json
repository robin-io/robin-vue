    "cypress": "^10.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^19.0.0",
    "prettier": "2.8.7",
    "typescript": "^4.3.2",
    "vite": "^2.5.1",
    "vite-plugin-vue2": "^1.8.1",
    "vue": "^2.6.14",
