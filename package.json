    "mime": "^2.5.2",
    "robin.io-js": "^0.4.0",
    "v-emoji-picker": "^2.3.3",
    "v-viewer": "^1.6.4",
    "vue-class-component": "^7.2.6"
  },
  "peerDependencies": {
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
