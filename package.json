{
  "name": "robin-vue",
  "version": "0.5.2",
  "files": [
    "dist"
  ],
  "keywords": [
    "robin.io",
    "vue"
  ],
  "module": "./dist/robin-vue.es.js",
  "types": "./dist/types/main.d.ts",
