{
  "name": "robin-vue",
  "version": "0.2.3",
  "files": [
    "dist"
  ],
  "keywords": [
    "robin.io",
    "vue"
  ],
  "main": "./dist/robin-vue.umd.js",
  "module": "./dist/robin-vue.es.js",
  "types": "./dist/types/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/robin-vue.es.js",
      "require": "./dist/robin-vue.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && vue-tsc --emitDeclarationOnly",
    "serve": "vite preview --port 8080",
    "lint": "prettier --write *.{js,ts,vue} --no-error-on-unmatched-pattern . && eslint --fix --ext .js,.ts,.vue --no-error-on-unmatched-pattern .",
    "test:unit": "vitest --environment jsdom",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "audio-recorder-polyfill": "^0.4.1",
    "axios": "^1.1.3",
    "i": "^0.3.7",
    "isomorphic-ws": "^5.0.0",
    "link-prevue": "^1.2.0",
    "localforage": "^1.10.0",
    "lodash.debounce": "^4.0.8",
    "mime": "^2.5.2",
    "moment": "^2.29.1",
    "npm": "^8.19.2",
    "v-emoji-picker": "^2.3.3",
    "v-lazy-image": "^2.0.1",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-toast-notification": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.2.0",
    "@testing-library/vue": "^5.8.2",
    "@types/jest": "^28.1.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/test-utils": "^1.3.0",
    "babel-loader": "^8.2.2",
    "cypress": "^10.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^19.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.1",
    "vite-plugin-vue2": "^1.8.1",
    "vitest": "^0.9.3",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.38.8"
  }
}
